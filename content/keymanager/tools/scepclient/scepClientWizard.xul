<!DOCTYPE wizard
	[
	<!ENTITY % certManager.dtd SYSTEM "chrome://pippki/locale/certManager.dtd">
	%certManager.dtd;
	<!ENTITY % keyManager.dtd SYSTEM "chrome://keymanager/locale/keyManager.dtd">
	%keyManager.dtd;
	]>

<wizard id="keymgr.scepclient.wizard"
    windowtype="avaya:scepclient"
    xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
    title="&keymgr.scepclient.wizard.title;"
    flex="1"
    onwizardfinish="return scepClientWizardFinish(this);"
    onwizardcancel="return scepClientWizardCancel(this);"
    persist="width height screenX screenY"
    >

    <!--
    onload="initScepClientWizard();"
    style="width: 48em; height: 48em;"
    -->
    <script type="application/x-javascript" src="chrome://global/content/strres.js"/>
  
    <script type="application/x-javascript" src="chrome://keymanager/content/XULFileUtil.js"/>
    <script type="application/x-javascript" src="chrome://keymanager/content/certutil.js"/>
    <script type="application/x-javascript" src="chrome://keymanager/content/BrowserPrefUtil.js"/>

    <script type="application/x-javascript" src="chrome://keymanager/content/httputil.js"/>

    <script type="application/x-javascript" src="chrome://keymanager/content/tools/scepclient/JsSCEPClient.js"/>
    <script type="application/x-javascript" src="chrome://keymanager/content/tools/scepclient/ScepGetCertInitialData.js"/>
    <script type="application/x-javascript" src="chrome://keymanager/content/tools/scepclient/scepClientWizard.js"/>
    <script type="application/x-javascript" src="chrome://keymanager/content/tools/scepclient/scepClientFormReqMsg.js"/>

    <wizardpage
      pageid="keymgr.scepclient.msgtype.wizardpage"
      description="&keymgr.scepclient.msgtype.wizardpage.label;"
      onpageshow="return msgTypePageShow();"
      onpageadvanced="return msgTypePageAdvanced();"
      next="keymgr.scepclient.pkcsreq.user.wizardpage"
      >
	      <grid flex="1"> 
	        <columns>
	           <column/>
	           <column flex="1"/>
	           <column flex="10"/>
	        </columns>
	        <rows>
	          <row
		    align="center"
		    >
	            <label value="&keymgr.scepclient.msgtype.label;"/> 
		    <hbox
		      pack="start"
		      >
	              <menulist id="keymgr.scepclient.msgtype.menuList"
			persist="value"
  		        tooltiptext="&keymgr.scepclient.msgtype.menuList.tooltip;"
		        readonly="true"
		        oncommand="handleWizardScepReqMsgTypeChange(this, event.target)"
		        >
	                <menupopup>
	                  <menuitem
			      id="keymgr.scepclient.msgtype.GetCACert"
			      label="&keymgr.scepclient.msgtype.GetCACert.label;"
			      value="GetCACert"
			      selected="true"
			     />
	                  <menuitem
			      id="keymgr.scepclient.msgtype.PKCSReq"
			      label="&keymgr.scepclient.msgtype.PKCSReq.label;"
			      value="PKCSReq"
			     />
	                  <menuitem
			      id="keymgr.scepclient.msgtype.GetCertInitial"
			      label="&keymgr.scepclient.msgtype.GetCertInitial.label;"
			      value="GetCertInitial"
			     />
	                  <menuitem
			      id="keymgr.scepclient.msgtype.GetCert"
			      label="&keymgr.scepclient.msgtype.GetCert.label;"
			      value="GetCert"
			     />
	                  <menuitem
			      id="keymgr.scepclient.msgtype.GetCRL"
			      label="&keymgr.scepclient.msgtype.GetCRL.label;"
			      tooltiptext="&keymgr.scepclient.msgtype.GetCRL.tooltip;"
			      disabled="true"
			      value="GetCRL"
			     />
			  <!--
	                  <menuitem
			      id="keymgr.scepclient.msgtype.CertRep"
			      label="&keymgr.scepclient.msgtype.CertRep.label;"
			      value="CertRep"
			     />
		          -->
	                </menupopup>
	              </menulist>
		    </hbox>
	          </row>

	          <row hidden="true">
		    <spacer/>
	            <label id="keymgr.scepclient.msgtype.wizardpage.next.GetCACert"
	                value="&keymgr.scepclient.msgtype.wizardpage.next.GetCACert.label;"
			hidden="true"/>
	            <label id="keymgr.scepclient.msgtype.wizardpage.next.PKCSReq"
	                value="&keymgr.scepclient.msgtype.wizardpage.next.PKCSReq.label;"
			hidden="true"/>
	            <label id="keymgr.scepclient.msgtype.wizardpage.next.GetCertInitial"
	                value="&keymgr.scepclient.msgtype.wizardpage.next.GetCertInitial.label;"
			hidden="true"/>
	            <label id="keymgr.scepclient.msgtype.wizardpage.next.GetCert"
	                value="&keymgr.scepclient.msgtype.wizardpage.next.GetCert.label;"
			hidden="true"/>
	            <label id="keymgr.scepclient.msgtype.wizardpage.next.GetCRL"
	                value="&keymgr.scepclient.msgtype.wizardpage.next.GetCRL.label;"
			hidden="true"/>
	          </row>
	        </rows>
	      </grid>
    </wizardpage>

  <wizardpage
    pageid="keymgr.scepclient.pkcsreq.user.wizardpage"
    description="&keymgr.scepclient.pkcsreq.user.wizardpage.label;"
    flex="1"
    onpageshow="return pkcsReqUserCertPageShow();"
    onpageadvanced="return pkcsReqUserCertPageAdvanced();"
    next="keymgr.scepclient.pkcsreq.issuer.wizardpage"
    >
    <!--
    next="keymgr.scepclient.pkcsreq.csr.wizardpage"
    -->
    <groupbox id="keymgr.scepclient.pkcsreq.user.inputparams">
	<caption class="header" label="&keymgr.scepclient.pkcsreq.user.inputparams.label;"/>
	<grid> 
	    <columns>
	        <column/>
	        <column/>
	        <column flex="10"/>
	    </columns>
	    <rows>
	        <row
		    align="center"
		    >
		    <spacer/>
		    <hbox
		    	pack="start"
		        align="start"
		        >
	                <checkbox id="keymgr.scepclient.pkcsreq.user.cert.renew"
		            label="&keymgr.scepclient.pkcsreq.user.cert.renew.label;"
  		      	    tooltiptext="&keymgr.scepclient.pkcsreq.user.cert.renew.tooltip;"
		    	    oncommand="handlePKCSReqUseCertRenewChange(this, event);"
			    /> 
		      </hbox>
	        </row>

	        <row
		    align="center"
		    >
	            <label value="&keymgr.scepclient.pkcsreq.user.cert.label;"/> 
		    <hbox
		        pack="start"
		        align="start"
		        >
	                <certpicker id="keymgr.scepclient.pkcsreq.user.certpicker"
  		            menutooltiptext="&keymgr.scepclient.pkcsreq.user.cert.tooltip;"
			    tokentypehidden="true"
			    certtype="user_selfsigned"
			    certtypehidden="true"
			    certusage="EmailRecipient"
			    certusagehidden="false"
			    certusagedisabled="true"
		            viewhidden="false"
	  	            viewlabel="&keymgr.scepclient.wizard.cert.view.label;"
  		            viewtooltiptext="&keymgr.scepclient.pkcsreq.user.cert.view.tooltip;"
		            includeTokenInfo="true"
	       	            oncommand="handlePKCSReqUserCertPickerChange(this, event);"
	    	            />
	    		<createcert id="keymgr.scepclient.pkcsreq.user.createcert"
			    class="menu"
			    optypelabel="&keymgr.scepclient.pkcsreq.user.cert.create.label;"
			    casignedhidden="true"
			    selfproxyhidden="true"
			    otpkhidden="true"
			    oncommand="handlePKCSReqUserCertCreate(this, event)"
			    />
		      </hbox>
	          </row>

	          <row
		      align="center"
		      >
	              <label value="&keymgr.scepclient.pkcsreq.user.certreq.challengepw.label;"/>
		      <hbox pack="start">
			  <!--
			  Note: Avaya Device profile menu is hack to support SCEP enrollment 
			        using Avaya TM for unregitered devices and user.
				By default, this menu item element is not displayed. 
				Use the 'keymgr.scepclient.avdevprofile.enabled' perference to enable it.
			  -->
	                  <menulist id="keymgr.scepclient.pkcsreq.user.certreq.avdevprofiles"
			    xpersist="value"
			    hidden="true"
  		            tooltiptext="&keymgr.scepclient.pkcsreq.user.certreq.avdevprofiles.tooltip;"
		            xreadonly="true"
		            xoncommand="handleAvDeviceProfileChange(this, event.target)"
		            >
			    <menupopup>
	                        <menuitem
			            id="keymgr.scepclient.pkcsreq.user.certreq.avdevprofile.none"
			            label="&keymgr.scepclient.pkcsreq.user.certreq.avdevprofile.none.label;"
				    selected="true"
			            value=""
			           />
	                        <menuitem
			            id="keymgr.scepclient.pkcsreq.user.certreq.avdevprofile.inouttls"
			            label="&keymgr.scepclient.pkcsreq.user.certreq.avdevprofile.inouttls.label;"
			            value="INBOUND_OUTBOUND_TLS"
			           />
	                        <menuitem
			            id="keymgr.scepclient.pkcsreq.user.certreq.avdevprofile.intls"
			            label="&keymgr.scepclient.pkcsreq.user.certreq.avdevprofile.intls.label;"
			            value="INBOUND_TLS"
			           />
	                        <menuitem
			            id="keymgr.scepclient.pkcsreq.user.certreq.avdevprofile.outtls"
			            label="&keymgr.scepclient.pkcsreq.user.certreq.avdevprofile.outtls.label;"
			            value="OUTBOUND_TLS"
			           />
			    </menupopup>
	                  </menulist>
	            	  <textbox id="keymgr.scepclient.pkcsreq.user.certreq.challengepw"
			      type="password"
			      tooltiptext="&keymgr.scepclient.pkcsreq.user.certreq.challengepw.tooltip;"
		    	      />
	      	          <button id="keymgr.scepclient.pkcsreq.user.challengepw.retrieve"
			      label="&keymgr.scepclient.pkcsreq.user.challengepw.retrieve.label;"
  		      	      tooltiptext="&keymgr.scepclient.pkcsreq.user.challengepw.retrieve.tooltip;"
			      oncommand="getUserChallengePassword(event);"
			      /> 
		      </hbox>
	          </row>

	          <row>
	              <separator class="thin"/>
	              <spacer/>
	          </row>
	    </rows>
	</grid>
    </groupbox>

    <groupbox>
	<caption class="header" label="&keymgr.scepclient.pkcsreq.user.certreq.label;"/>
	<grid> 
	    <columns>
	        <column/>
	        <column flex="1"/>
	    </columns>
	    <rows>
		<row
		    align="center"
		    >
	            <label value="&keymgr.scepclient.pkcsreq.user.certreq.file.label;"/>
	            <hbox>
			<fileselector id="keymgr.scepclient.pkcsreq.user.certreq.filepicker" flex="2"
			    ascii="true"
  			    formatlabel="&keymgr.scepclient.wizard.file.format.base64.label;"
  			    formattooltiptext="&keymgr.scepclient.wizard.file.format.base64.tooltip;"
			    fileMode="save"
			    fileDialogMsg="Choose PKCS#10 CSR File for User Key"
			    fileTypeMsg="PKCS#10 CSR File"
			    fileTypeFilters="*.csr; *.p10"
	  		    browselabel="&keymgr.scepclient.wizard.file.browse.label;"
	  		    browsetooltiptext="&keymgr.scepclient.wizard.file.browse.tooltip;"
	        	    />
	            </hbox>
	        </row>

		<row hidden="row">
		    <spacer/>
	            <label id="keymgr.scepclient.pkcsreq.user.wizardpage.next"
	                value="&keymgr.scepclient.pkcsreq.user.wizardpage.next.label;"
			hidden="true"/>
	        </row>
	    </rows>
	</grid>
    </groupbox>
  </wizardpage>

