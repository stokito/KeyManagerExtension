  <wizardpage
    pageid="keymgr.scepclient.pkcsreq.issuer.wizardpage"
    description="&keymgr.scepclient.pkcsreq.issuer.wizardpage.label;"
    flex="1"
    onpageshow="return pkcsReqIssuerCertPageShow();"
    onpageadvanced="return pkcsReqIssuerCertPageAdvanced();"
    next="keymgr.scepclient.pkcsreq.scepcsrparams.wizardpage"
    >
	    <groupbox>
	      <grid flex="1"> 
	        <columns>
	           <column/>
	           <column flex="1"/>
	        </columns>
	        <rows>
	          <row
		    align="center"
		    >
	            <label control="keymgr.scepclient.pkcsreq.issuer.server.url"
	              value="&keymgr.scepclient.pkcsreq.issuer.server.url.label;"/>
	            <hbox 
		      pack="start"
		      >
  	              <textbox id="keymgr.scepclient.pkcsreq.issuer.server.url" flex="1"
  		        tooltiptext="&keymgr.scepclient.pkcsreq.issuer.server.url.tooltip;"
			persist="value"
  	  	        type="autocomplete" 
  	  	        autocompletesearch="form-history" 
  	  	        completedefaultindex="true"
  	  	        completeselectedindex="true"
		        enablehistory="true"
  	  	        autocompletesearchparam="keymgr.scepclient.pkcsreq.issuer.server.url"
		        onchange="handlePKCSReqScepServerURLChange(this, event)"
		        onpopuphiding="handlePKCSReqScepServerURLChange(this, event)"
  		        />
	            </hbox>
	          </row>
	          <row
		    align="center"
		    >
		    <spacer/>
		    <hbox
		    	pack="start"
			>
			<checkbox id="keymgr.scepclient.pkcsreq.issuer.recipient.isra"
			    label="&keymgr.scepclient.pkcsreq.issuer.recipient.isra.label;"
			    tooltiptext="&keymgr.scepclient.pkcsreq.issuer.recipient.isra.tooltip;"
	       	            oncommand="handlePKCSReqRAAsMsgRecipientChange(this, event);"
			    />
		    </hbox>
	          </row>

		  <row id="keymgr.scepclient.pkcsreq.issuer.getinfo.row"
		      align="center"
		      >
		      <label control="keymgr.scepclient.pkcsreq.issuer.caid"
			  value="&keymgr.scepclient.basic.scepserver.caid.label;"
			  />
		      <hbox 
			  pack="start"
			  >
			  <textbox id="keymgr.scepclient.pkcsreq.issuer.caid"
			      tooltiptext="&keymgr.scepclient.basic.scepserver.caid.tooltip;"
			      xonchange="handleGetCACertCAIdChange(this, event)"
			      />
	      	          <button id="keymgr.scepclient.pkcsreq.issuer.server.getcaps"
	                      label="&keymgr.scepclient.pkcsreq.issuer.server.getcaps.label;"
  		      	      tooltiptext="&keymgr.scepclient.pkcsreq.issuer.server.getcaps.tooltip;"
	                      oncommand="scepGetCACapabilities('keymgr.scepclient.pkcsreq.issuer.server.url', event);"
			      /> 
			  <button id="keymgr.scepclient.pkcsreq.issuer.downloadcacert"
			      label="&keymgr.scepclient.basic.scepserver.downloadcert.label;"
			      tooltiptext="&keymgr.scepclient.basic.scepserver.downloadcert.tooltip;"
	                      oncommand="scepDownloadCACert(event);"
			      /> 
			  <!--
	      	          <button id="keymgr.scepclient.basic.scepserver.getcaps"
			      label="&keymgr.scepclient.basic.scepserver.getcaps.label;"
  		      	      tooltiptext="&keymgr.scepclient.basic.scepserver.getcaps.tooltip;"
			      oncommand="SCEPClientBasicForm.scepGetCACapabilities(event);"
			      /> 
			  -->
		      </hbox> 
		  </row>

	          <row>
	            <separator class="thin"/>
	            <spacer/>
	          </row>

	        </rows>
	      </grid>
	    </groupbox>

	    <groupbox id="keymgr.scepclient.pkcsreq.issuer.inputparams">
	      <caption class="header" label="&keymgr.scepclient.pkcsreq.issuer.inputparams.label;"/>
	      <grid flex="1"> 
	        <columns>
	           <column/>
	           <column flex="1"/>
	        </columns>
	        <rows>
	          <row
		    align="center"
		    >
	            <label value="&keymgr.scepclient.pkcsreq.issuer.cert.label;"/> 
		    <hbox>
	                <certpicker id="keymgr.scepclient.pkcsreq.issuer.certpicker"
  		            menutooltiptext="&keymgr.scepclient.pkcsreq.issuer.cert.tooltip;"
			    tokentypehidden="true"
			    certtype="ca"
			    certtypehidden="true"
			    certusage="VerifyCA"
			    certusagehidden="false"
			    certusagedisabled="true"
	  	            viewlabel="&keymgr.scepclient.wizard.cert.view.label;"
		            viewhidden="false"
  		            viewtooltiptext="&keymgr.scepclient.wizard.cert.view.tooltip;"
		            includeTokenInfo="true"
	       	            oncommand="handlePKCSReqIssuerCertPickerChange(this, event);"
	    	            />
		    </hbox>
	          </row>

	          <row
		    align="center"
		    >
	            <label value="&keymgr.scepclient.pkcsreq.issuer.recipient.cert.label;"/> 
		    <hbox
		    	pack="start"
			>
	                <certpicker id="keymgr.scepclient.pkcsreq.issuer.recipient.certpicker"
  		            menutooltiptext="&keymgr.scepclient.pkcsreq.issuer.recipient.cert.tooltip;"
			    tokentypehidden="true"
			    certtype="ca"
			    xcerttypedisabled="true"
			    xcerttypehidden="true"
			    certusage="VerifyCA"
			    certusagehidden="false"
			    xcertusagedisabled="true"
	  	            viewlabel="&keymgr.scepclient.wizard.cert.view.label;"
		            viewhidden="false"
  		            viewtooltiptext="&keymgr.scepclient.wizard.cert.view.tooltip;"
		            includeTokenInfo="true"
	       	            oncommand="handlePKCSReqScepRecipientCertPickerChange(this, event);"
	    	            />
		    </hbox>
	          </row>

		  <!--
	          <row>
	            <label value="&keymgr.scepclient.pkcsreq.issuer.cert.subjdn.label;"/> 
	            <label id="keymgr.scepclient.pkcsreq.issuer.cert.subjdn" value=""/>
	          </row>
	          <row>
	            <label value="&keymgr.issuerinfo.label;"/>
	            <label id="keymgr.scepclient.pkcsreq.issuer.cert.issuedby"/>
	          </row>
	          <row>
	            <label value="&keymgr.certdetail.serialnumber;"/> 
	            <label id="keymgr.scepclient.pkcsreq.issuer.cert.serialnumber"/>
	          </row>
	          <row>
	            <label value="&keymgr.expires;"/>
	            <label id="keymgr.scepclient.pkcsreq.issuer.cert.validityend"/>
	          </row>
		  -->

	          <row hidden="true">
		    <spacer/>
	            <label id="keymgr.scepclient.pkcsreq.issuer.wizardpage.next"
	                value="&keymgr.scepclient.pkcsreq.issuer.wizardpage.next.label;"
			hidden="true"/>
	          </row>
	        </rows>
	      </grid>
	    </groupbox>

  </wizardpage>

  <wizardpage
    pageid="keymgr.scepclient.pkcsreq.scepcsrparams.wizardpage"
    description="&keymgr.scepclient.pkcsreq.scepcsrparams.wizardpage.label;"
    flex="1"
    onpageshow="return pkcsReqScepCsrPageShow();"
    onpageadvanced="return pkcsReqScepCsrPageAdvanced();"
    next="keymgr.scepclient.pkcsreq.scepreqmsg.wizardpage"
    >
	    <groupbox id="keymgr.scepclient.pkcsreq.scepcsrparams">
	      <caption class="header" label="&keymgr.scepclient.pkcsreq.scepcsrparams.param.label;"/>
	      <grid flex="1"> 
	        <columns>
	           <column/>
	           <column flex="1"/>
	        </columns>
	        <rows>
	          <row
		    align="center"
		    >
	            <label value="&keymgr.scepclient.pkcsreq.scepcsrparams.user.cert.label;"/> 
		    <certitem id="keymgr.scepclient.pkcsreq.scepcsrparams.user.cert" 
	  		viewlabel="&keymgr.scepclient.wizard.cert.view.label;"
  		      	viewtooltiptext="&keymgr.scepclient.wizard.cert.view.tooltip;"
		        />
	          </row>
	          <row
